<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>CORS</filter-name>
  	<display-name>CORS</display-name>
  	<description></description>
  	<filter-class>com.example.servlets.CorsFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CORS</filter-name>
  	<url-pattern>/CORS</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>post</servlet-name>
  	<display-name>post</display-name>
  	<description></description>
  	<servlet-class>com.example.AdminServlets.post</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>post</servlet-name>
  	<url-pattern>/post</url-pattern>
  </servlet-mapping>
</web-app>
